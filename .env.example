# Environment Configuration
ENVIRONMENT=development

# API Keys
OPENAI_API_KEY=your_openai_api_key
NEWS_API_KEY=your_news_api_key  # Optional for Web Search Agent
PINECONE_API_KEY=your_pinecone_api_key

# Redis Configuration (Optional)
# If not provided, system will use in-memory storage
REDIS_URL=redis://username:password@host:port
REDIS_HOST=your_redis_host
REDIS_PORT=your_redis_port
REDIS_PASSWORD=your_redis_password
REDIS_USERNAME=your_redis_username
REDIS_SSL=true

# OpenAI Configuration
OPENAI_MODEL=gpt-3.5-turbo  # or gpt-4
EMBEDDING_MODEL=text-embedding-3-small

# Pinecone Configuration
PINECONE_ENVIRONMENT=us-east1-gcp  # your Pinecone environment (e.g., gcp-starter, us-east1-gcp, etc.)
PINECONE_INDEX=medical-supply-chain
PINECONE_DIMENSION=384
PINECONE_METRIC=cosine
PINECONE_REGION=us-east-1
PINECONE_CLOUD=aws

# Web Server Configuration
PORT=5000
HOST=0.0.0.0
DEBUG=True

# Document Storage
DOCUMENT_STORE_PATH=document_store

# RAG Configuration
CHUNK_SIZE=512
CHUNK_OVERLAP=50
TOP_K=5
SIMILARITY_THRESHOLD=0.7
VECTOR_DIMENSION=384

# Agent Configuration
AGENT_ENABLED=True
AGENT_MAX_ITERATIONS=5

# Optional LangSmith Configuration
#LANGSMITH_API_KEY=your_langsmith_api_key
#LANGSMITH_PROJECT_NAME=your_project_name
#LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Logging
LOG_LEVEL=INFO

# Batch Processing Configuration
BATCH_SIZE=100 